<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="LightTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowLightStatusBar">true</item>
        <item name="colorPrimary">#e0e0e0</item>
        <item name="colorPrimaryDark">#FAFAFA</item>
        <item name="colorAccent">#f7efef</item>
    </style>
    <style name="registerTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">#F0F0F0</item>  <!--状态栏背景颜色-->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
</resources>