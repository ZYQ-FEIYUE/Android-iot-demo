<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="4000"
        motion:motionInterpolator="easeInOut">
        <OnClick
            motion:clickAction="toggle"
            motion:targetId="@id/start_image1"/>
        <KeyFrameSet>
            <KeyPosition
                motion:framePosition="24"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image1"
                motion:pathMotionArc="flip"
                motion:percentX="0.25"
                motion:percentY="0.65"/>
            <KeyPosition
                motion:framePosition="40"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image1"
                motion:pathMotionArc="flip"
                motion:percentX="0"
                motion:percentY="0.5"/>
            <KeyPosition
                motion:framePosition="56"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image1"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.25"/>
            <KeyPosition
                motion:framePosition="72"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image1"
                motion:pathMotionArc="flip"
                motion:percentX="1"
                motion:percentY="0.5"/>
            <KeyPosition
                motion:framePosition="88"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image1"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.75"/>

            <KeyPosition
                motion:framePosition="20"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image2"
                motion:pathMotionArc="flip"
                motion:percentX="0.25"
                motion:percentY="0.65"/>
            <KeyPosition
                motion:framePosition="36"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image2"
                motion:pathMotionArc="flip"
                motion:percentX="0"
                motion:percentY="0.5"/>
            <KeyPosition
                motion:framePosition="52"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image2"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.25"/>
            <KeyPosition
                motion:framePosition="68"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image2"
                motion:pathMotionArc="flip"
                motion:percentX="1"
                motion:percentY="0.5"/>
            <KeyPosition
                motion:framePosition="84"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image2"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.75"/>

            <KeyPosition
                motion:framePosition="16"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image3"
                motion:pathMotionArc="flip"
                motion:percentX="0.25"
                motion:percentY="0.65" />
            <KeyPosition
                motion:framePosition="32"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image3"
                motion:pathMotionArc="flip"
                motion:percentX="0"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="48"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image3"
                motion:pathMotionArc="startHorizontal"
                motion:percentX="0.5"
                motion:percentY="0.25" />
            <KeyPosition
                motion:framePosition="64"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image3"
                motion:pathMotionArc="startVertical"
                motion:percentX="1"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="80"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image3"
                motion:pathMotionArc="none"
                motion:percentX="0.5"
                motion:percentY="0.75" />

            <KeyPosition
                motion:framePosition="12"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image4"
                motion:pathMotionArc="flip"
                motion:percentX="0.25"
                motion:percentY="0.65" />
            <KeyPosition
                motion:framePosition="28"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image4"
                motion:pathMotionArc="flip"
                motion:percentX="0"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="44"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image4"
                motion:pathMotionArc="startHorizontal"
                motion:percentX="0.5"
                motion:percentY="0.25" />
            <KeyPosition
                motion:framePosition="60"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image4"
                motion:pathMotionArc="startVertical"
                motion:percentX="1"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="76"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image4"
                motion:pathMotionArc="none"
                motion:percentX="0.5"
                motion:percentY="0.75" />

            <KeyPosition
                motion:framePosition="8"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image5"
                motion:pathMotionArc="flip"
                motion:percentX="0.25"
                motion:percentY="0.65" />
            <KeyPosition
                motion:framePosition="24"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image5"
                motion:pathMotionArc="flip"
                motion:percentX="0"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="40"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image5"
                motion:pathMotionArc="startHorizontal"
                motion:percentX="0.5"
                motion:percentY="0.25" />
            <KeyPosition
                motion:framePosition="56"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image5"
                motion:pathMotionArc="startVertical"
                motion:percentX="1"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="72"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image5"
                motion:pathMotionArc="none"
                motion:percentX="0.5"
                motion:percentY="0.75" />

            <KeyPosition
                motion:framePosition="4"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image6"
                motion:pathMotionArc="flip"
                motion:percentX="0.25"
                motion:percentY="0.65" />
            <KeyPosition
                motion:framePosition="20"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image6"
                motion:pathMotionArc="flip"
                motion:percentX="0"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="36"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image6"
                motion:pathMotionArc="startHorizontal"
                motion:percentX="0.5"
                motion:percentY="0.25" />
            <KeyPosition
                motion:framePosition="52"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image6"
                motion:pathMotionArc="startVertical"
                motion:percentX="1"
                motion:percentY="0.5" />
            <KeyPosition
                motion:framePosition="68"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/start_image6"
                motion:pathMotionArc="none"
                motion:percentX="0.5"
                motion:percentY="0.75" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/start_image1"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:drawPath="path"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:pathMotionArc="startVertical" />

        <Constraint
            android:id="@+id/start_image2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:drawPath="path"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:pathMotionArc="startVertical" />
        <Constraint
            android:id="@+id/start_image3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:drawPath="path"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:pathMotionArc="startVertical" />
        <Constraint
            android:id="@+id/start_image4"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:drawPath="path"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:pathMotionArc="startVertical" />
        <Constraint
            android:id="@+id/start_image5"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:drawPath="path"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:pathMotionArc="startVertical" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/start_image1"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="@id/start_image3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toStartOf="@id/start_image2"
            motion:layout_constraintVertical_chainStyle="spread_inside" />
        <Constraint
            android:id="@+id/start_image2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="@id/start_image3"
            motion:layout_constraintStart_toEndOf="@id/start_image1"
            motion:layout_constraintEnd_toStartOf="@id/start_image3" />
        <Constraint
            android:id="@+id/start_image3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginBottom="20dp"
            motion:layout_constraintBottom_toTopOf="@id/start_image4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />

        <Constraint
            android:id="@+id/start_image4"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:id="@+id/start_image5"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="@id/start_image4"
            motion:layout_constraintEnd_toStartOf="@id/start_image6"
            motion:layout_constraintVertical_chainStyle="spread_inside" />
        <Constraint
            android:id="@+id/start_image6"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toEndOf="@id/start_image5"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>
</MotionScene>